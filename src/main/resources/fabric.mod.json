    "sources": "https://github.com/MeteorDevelopment/meteor-addon-template/",
    "discord": "https://meteorclient.com/discord"
  },

  "license": "GPL-3.0",
  "icon": "assets/example/icon.png",

  "environment": "client",
  "entrypoints": {
    "meteor": [
      "meteordevelopment.addons.example.ExampleAddon"
    ]
  },
  "mixins": [
    "example.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x",
    "meteor-client": "*"
